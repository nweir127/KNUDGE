    {
      "id": 187,
      "speaker_guid": "player",
      "speaker": "Player",
      "utterance": "You're definitely going to use it.",
      "support_knowledge": []
    },
    {
      "id": 190,
      "speaker_guid": "526f2b56-5602-4316-a480-e8a6f5adcdbb",
      "speaker": "VaughnCortes (Male)",
      "utterance": "Take this. You earned it. If you run into me again after all this is over, maybe I'll have more.",
      "support_knowledge": []
    },
    {
      "id": 196,
      "speaker_guid": "526f2b56-5602-4316-a480-e8a6f5adcdbb",
      "speaker": "VaughnCortes (Male)",
      "utterance": "Pleasure doing business with you.",
